<h1>Genre</h1>

<div>
  <p>Genre: <%= @genre.name %></p>


  <p>Artists:
    <% @genre.artists.each do |artist| %>
      <a href="/artists/<%= artist.slug%>"><%= artist.name %></a>&nbsp;
    <% end %>
  </p>

  <p>Songs:
    <% @genre.songs.each do |song| %>
    <a href="/songs/<%= song.slug %>"><%= song.name %></a>&nbsp;
    <% end %>
  </p>

</div>
