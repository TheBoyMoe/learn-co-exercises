<h1><%= @patient.name %></h1>
<ul>
  <% @patient.appointments.each do |appointment| %>
    <li><%= link_to appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M"), appointment_path(appointment) %> with doctor <%= link_to appointment.doctor.name, doctor_path(appointment.doctor)%></li>
  <% end %>
</ul>
